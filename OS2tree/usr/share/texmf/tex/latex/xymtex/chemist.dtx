\@tempcnta=\@tempdima \@tempcntb=\unitlength
\divide\@tempcnta by \@tempcntb
\edef\kanzanchi{\the\@tempcnta}%
\divide\@tempcnta by2\relax
\edef\kanzanhalf{\the\@tempcnta}}
