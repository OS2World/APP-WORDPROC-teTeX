   \else\if\@tmpb s\relax
     \@bclipfalse%
     \putratom{175}{303}{\@memberb}%  % right type
   \else\if\@tmpb a\relax%omit fused position
   \else
     \@bcliptrue%
     \putratom{135}{280}{\@memberb}%  % right type
   \fi\fi\fi
