   \else\if\@tmpb s\relax
     \@clipfalse%
     \putratom{0}{0}{\@memberb}%  % right type
   \else\if\@tmpb a\relax%omit fused position
   \else
     \@cliptrue%
     \putratom{-31}{-10}{\@memberb}%  % right type
   \fi\fi\fi
\or%position 5
  \if\@tmpb h\relax
