     \@eclipfalse%
     \putlatom{-175}{103}{\@memberb}%  % right type
   \else\if\@tmpb a\relax%omit fused position
   \else
     \@ecliptrue%
     \putlatom{-135}{65}{\@memberb}%  % left type
   \fi\fi\fi
\or%position 6
  \if\@tmpb h\relax
     \@fcliptrue%
