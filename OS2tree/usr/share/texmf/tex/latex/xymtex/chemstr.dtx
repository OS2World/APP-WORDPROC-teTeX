   \else\if\@tmpb s\relax
     \@fclipfalse%
     \putlatom{-175}{303}{\@memberb}%  % right type
   \else\if\@tmpb a\relax%omit fused position
   \else
     \@fcliptrue%
     \putlatom{-135}{280}{\@memberb}% % left type
   \fi\fi\fi
