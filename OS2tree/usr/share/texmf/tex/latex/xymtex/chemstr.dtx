% \changes{v2.00}{1998/11/15}{New command: \cs{test@vertix} and 
% \cs{test@vertix@sixv}}
%    \begin{macrocode}
\def\test@vertix#1{%
   \expandafter\def\csname #1#1#1\endcsname{@}%
   \if\@tmpb h\relax
    \expandafter\def\csname #1#1#1\endcsname{@}%
   \else\if\@tmpb s\relax
   \expandafter\def\csname #1#1#1\endcsname{}%
   \fi\fi}
%    \end{macrocode}
