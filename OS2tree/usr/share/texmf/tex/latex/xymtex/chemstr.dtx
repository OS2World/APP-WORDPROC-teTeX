% % \put(#1,#2){\hbox to.72em{#3}}\global\@moietyfalse
% % \else\put(#1,#2){\hbox to.6em{#3\hss}}\fi}%
% % \def\putlratom#1#2#3{\put(#1,#2){\hbox to.6em{#3}}}
