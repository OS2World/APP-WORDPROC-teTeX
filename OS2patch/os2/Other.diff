@@ -0,0 +1,101 @@
+#########################################################################
+# Makefile.dll to make kpath354.dll
+#                      Jun Sawataishi <jsawa@attglobal.net>
+#                                               31-Dec-2004
+#########################################################################
+#
+# all you have to do is set `emx_drive' according to your environment
+emx_drive	= g:
+
+
+os2_dll_dir = $(emx_drive)/emx/dll
+os2_lib_dir = $(emx_drive)/emx/lib
+os2_include_dir = $(emx_drive)/emx/include
+install_headers = *.h
+
+
+
+default: all
+O=.obj
+VPATH=.;
+
+SHELL=sh
+XCFLAGS= -Zomf  -O2  -Zmtd -DOS2   -D__EMX__ \
+	 -Zsysv-signals -fno-strength-reduce -I..
+CC=gcc
+
+# defs
+# @@ make-subÇÕéQè∆ÇµÇ»Ç¢ÇÊÇ§Ç…ÇµÇƒÇ¢Ç‹Ç∑
+##     include ../make-sub
+
+# Target
+DLLNAME=kpath354
+DLFLAGS= -Zcrtdll -Zdll  -Zomf -Zmtd -Zbin-files
+LFLAGS= -Zcrtdll -Zmtd -Zomf -Zbin-files
+AR=emxomfar
+RANLIB=emxomfar s
+lib_dir = .libs
+
+all: $(DLLNAME).dll $(lib_dir)/kpathsea.a $(lib_dir)/kpathsea.lib
+
+clean::
+	-$(RM)  $(DLLNAME).dll 
+veryclean:: clean
+	-$(RM)  $(DLLNAME).dll
+distclean:: veryclean
+	-$(RM)  $(DLLNAME).dll
+
+# Font drivers
+ALL_OBJ  = \
+tex-file.obj absolute.obj atou.obj cnf.obj concat.obj concat3.obj \
+concatn.obj db.obj debug.obj dir.obj elt-dirs.obj expand.obj extend-fname.obj \
+file-p.obj find-suffix.obj fn.obj fontmap.obj getopt.obj getopt1.obj hash.obj \
+kdefault.obj line.obj magstep.obj make-suffix.obj path-elt.obj \
+pathsearch.obj proginit.obj progname.obj readable.obj rm-suffix.obj \
+str-list.obj str-llist.obj tex-glyph.obj tex-hush.obj tex-make.obj \
+tilde.obj truncate.obj uppercasify.obj variable.obj version.obj xbasename.obj \
+xcalloc.obj xdirname.obj xfopen.obj xfseek.obj xftell.obj xgetcwd.obj xmalloc.obj \
+xopendir.obj xputenv.obj xrealloc.obj xstat.obj xstrdup.obj strcasecmp.obj
+
+%.obj: %.o
+	emxomf -o $@ $<
+
+# kpathsea
+kpathsea.def: $(ALL_OBJ)
+	echo LIBRARY kpath354 INITINSTANCE TERMINSTANCE > $@
+	echo DESCRIPTION \'kpathsea 3.5.4\' >> $@
+	echo DATA >> $@
+	echo MULTIPLE NONSHARED >> $@
+	echo STACKSIZE 65536 >> $@
+	echo EXPORTS >> $@
+	emxexp -u  -o $(ALL_OBJ) >> $@
+
+kpathsea: $(lib_dir)/kpathsea.lib $(lib_dir)/kpathsea.a
+
+$(lib_dir)/kpathsea.lib: kpathsea.def 
+		emximp -o $@ kpathsea.def
+
+$(lib_dir)/kpathsea.a:  kpathsea.def
+		emximp -o $@ kpathsea.def
+	
+#	$(AR) rc $@ $(ALL_OBJ)
+#	$(RANLIB) $@
+	
+$(DLLNAME).dll: kpathsea.def $(ALL_OBJ)
+	$(CC) kpathsea.def -o $@ $(ALL_OBJ) $(DLFLAGS)
+	cp.exe -uvp $(DLLNAME).dll ../web2c/
+	cp.exe -uvp $(DLLNAME).dll ../web2c/web2c/
+	if [ -f ../web2c/ptex-src-3.1.8 ] ; then  cp.exe -uvp $(DLLNAME).dll ../web2c/ptex-src-3.1.8/ ; fi
+
+install: $(lib_dir)/kpathsea.lib $(lib_dir)/kpathsea.lib $(DLLNAME).dll
+	cp.exe -vp $(lib_dir)/kpathsea.lib $(os2_lib_dir)/
+	cp.exe -vp $(lib_dir)/kpathsea.a $(os2_lib_dir)/
+	cp.exe -vp $(DLLNAME).dll $(os2_dll_dir)/
+	cp.exe -vp $(install_headers) $(os2_include_dir)/
+
+
+
+
